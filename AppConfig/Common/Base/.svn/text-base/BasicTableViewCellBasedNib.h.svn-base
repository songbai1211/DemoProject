#import <UIKit/UIKit.h>

@interface BasicTableViewCellBasedNib : UITableViewCell

+ (UINib *)nib;
+ (NSString *)nibName;

+ (NSString *)cellIdentifier;
+ (id)cellForTableView:(UITableView *)tableView fromNib:(UINib *)nib;

@end






/*************************************************************************
                使用说明
 1 子类化BasicTableViewCellBasedNib
 2 子类的XIB文件名与子类类名一致
 3 子类的XIB文件的单元格与类名一致
 4 表格单元格是NIB中的第一个对象
